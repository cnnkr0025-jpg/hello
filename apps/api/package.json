{
  "name": "@ai-stack/api",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --out-dir dist",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "seed": "tsx ../../scripts/seed.ts",
    "db:reset": "tsx ../../scripts/db-reset.ts"
  },
  "dependencies": {
    "@ai-stack/core": "workspace:*",
    "@ai-stack/providers": "workspace:*",
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^10.0.1",
    "@fastify/sse-v2": "^3.0.1",
    "@scalar/fastify-openapi": "^0.7.1",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/sdk-trace-node": "^1.22.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.47.0",
    "@prisma/client": "^5.13.0",
    "bullmq": "5.6.4",
    "dotenv": "^16.4.5",
    "fastify": "^4.27.0",
    "fastify-plugin": "^4.5.1",
    "fastify-zod": "^1.4.0",
    "ioredis": "^5.4.1",
    "pino": "^8.19.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "tsup": "^7.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.3"
  }
}
