{
  "name": "ai-studio-monorepo",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "turbo run format",
    "seed": "pnpm --filter @ai/api db:seed",
    "db:push": "pnpm --filter @ai/api prisma:push",
    "db:migrate": "pnpm --filter @ai/api prisma:migrate",
    "db:reset": "pnpm --filter @ai/api db:reset",
    "key:rotate": "pnpm --filter @ai/api key:rotate",
    "ci": "turbo run lint test build"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "turbo": "^1.12.3",
    "typescript": "^5.4.5"
  }
}
